{
	"name": "sheadawson/silverstripe-editlock",
	"description": "Prevents a CMS user from editing a record that another CMS user is currently editing",
	"type": "silverstripe-vendormodule",
	"keywords": ["silverstripe", "edit", "lock", "checkout"],
	"license": "BSD License",
	"homepage": "http://github.com/sheadawson/silverstripe-editlock",
	"authors": [
	{
		"name": "Shea Dawson",
		"email": "shea@silverstripe.com.au"
	}
	],

	"require":
	{
		"silverstripe/framework": "^4",
		"composer/installers": "*"
	},
	"support": {
		"issues": "https://github.com/sheadawson/silverstripe-editlock/issues"
	},
    "autoload": {
        "psr-4": {
            "sheadawson\\Editlock\\": "src/"
        }
    },
	"extra": {
		"installer-name": "editlock",
        "expose": [
            "sheadawson/editlock/images",
            "sheadawson/editlock/javascript"
        ]
	}
}
